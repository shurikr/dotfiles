# Account Settings -----

set from      = "mhn@ksu.edu"
set sendmail  = "/usr/local/bin/msmtp -a mhn"

# Default inbox.
set spoolfile = "+mhn-ksu.edu/INBOX"

# Other special folders.
set mbox      = "+mhn-ksu.edu/_archive"
set postponed = "+mhn-ksu.edu/Drafts"
set record    = "+mhn-ksu.edu/Sent Items"

color status yellow default
